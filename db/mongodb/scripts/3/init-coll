sh.enableSharding("db")

sh.shardCollection("db.catalog_returns", {cr_order_number: "hashed"})
sh.shardCollection("db.catalog_sales", {cs_order_number: "hashed"})
sh.shardCollection("db.customer", {c_customer_sk: "hashed"})
sh.shardCollection("db.date_dim", {d_date_sk: "hashed"})
sh.shardCollection("db.store_sales", {ss_ticket_number: "hashed"})
sh.shardCollection("db.store_returns", {sr_ticket_number: "hashed"})
sh.shardCollection("db.warehouse", {w_warehouse_sk: "hashed"})
sh.shardCollection("db.web_sales", {ws_order_number: "hashed"})
sh.shardCollection("db.customer_address", {ca_address_sk: "hashed"})